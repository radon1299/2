{
    "name": "shock-me-design/zaborovo-v2",
    "description": "Yii 2 Advanced Project for Zaborovo site",
    "keywords": ["yii2", "framework", "advanced", "project template"],
    "homepage": "https://zaborovo.ru",
    "type": "project",
    "license": "BSD-3-Clause",
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.4.0",
        "ext-json": "*",
        "ext-simplexml": "*",
        "ext-mbstring": "*",
        "ext-dom": "*",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "yiisoft/yii2-bootstrap4": "@dev",
        "yiisoft/yii2-swiftmailer": "~2.0.0 || ~2.1.0",
        "yiisoft/yii2-apidoc": "~2.1.0",
        "yiisoft/yii2-queue": "@dev",
        "yiisoft/yii2-imagine": "~2.1.0",
        "true/punycode": "~2.0",
        "dmstr/yii2-adminlte-asset": "dev-master",
        "2amigos/yii2-usuario": "~1.0",
        "mihaildev/yii2-elfinder": "*",
        "simialbi/yii2-schema-org": "^2.0.0",
        "kartik-v/yii2-editors": "@dev",
        "kartik-v/yii2-icons": "dev-master",
        "kartik-v/yii2-widgets": "dev-master",
        "kartik-v/yii2-bootstrap4-dropdown": "dev-master",
        "kartik-v/yii2-editable": "dev-master",
        "kartik-v/yii2-dynagrid": "dev-master",
        "kartik-v/yii2-field-range": "dev-master",
        "alexandernst/yii2-device-detect": "0.0.12",
        "skeeks/yii2-slug-behavior": "*",
        "2amigos/yii2-transliterator-helper": "*",
        "vova07/yii2-console-runner-extension": "*",
        "evgeniyrru/yii2-slick" : "*",
        "himiklab/yii2-sitemap-module" : "*",
        "rmrevin/yii2-minify-view": "^1.15",
        "bedezign/yii2-audit": "^1.0",
        "insolita/yii2-opcache": "~1.0",
        "wbraganca/yii2-dynamicform": "*",
        "yiithings/yii2-dotenv": "*"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "yiisoft/yii2-debug": "~2.1.0",
        "yiisoft/yii2-gii": "~2.1.0",
        "yiisoft/yii2-faker": "~2.0.0",
        "codeception/codeception": "^4.0",
        "codeception/module-asserts": "^1.0",
        "codeception/module-yii2": "^1.0",
        "codeception/module-filesystem": "^1.0",
        "codeception/verify": "~0.5.0 || ~1.1.0",
        "symfony/browser-kit": ">=2.7 <=4.2.4"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "scripts": {
        "post-create-project-cmd": [
            "yii\\composer\\Installer::setPermission"
        ]
    },
    "extra": {
        "writable": [
            "backend/runtime",
            "backend/web/assets",
            "frontend/runtime",
            "frontend/web/assets"
        ]
    }
}